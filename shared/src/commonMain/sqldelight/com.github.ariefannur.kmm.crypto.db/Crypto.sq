CREATE TABLE Coin (
id INTEGER NOT NULL PRIMARY KEY,
name TEXT NOT NULL,
symbol TEXT,
slug TEXT,
marketCap REAL,
price REAL,
move REAL,
imageLogo TEXT
);

insertCoin:
INSERT OR REPLACE INTO Coin(id, name, symbol, slug, price, marketCap, move, imageLogo) VALUES(?, ?, ?, ?, ?, ?, ?, ?);

selectAll:
SELECT * FROM Coin;



